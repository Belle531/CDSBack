    "serviceRawFile": "service: cds-spice-rack-api\r\n\r\nprovider:\r\n  name: aws\r\n  runtime: nodejs18.x\r\n  region: us-east-1\r\n  stage: ${opt:stage, 'dev'}\r\n  \r\n  environment:\r\n    USERS_TABLE: ${self:service}-users-${self:provider.stage}\r\n    JWT_SECRET: ${env:JWT_SECRET, 'fallback-secret-key'}\r\n\r\nfunctions:\r\n  register:\r\n    handler: src/handlers/auth.register\r\n    events:\r\n      - http:\r\n          path: auth/register\r\n          method: post\r\n          cors: true\r\n  \r\n  login:\r\n    handler: src/handlers/auth.login\r\n    events:\r\n      - http:\r\n          path: auth/login\r\n          method: post\r\n          cors: true\r\n\r\nplugins:\r\n  - serverless-offline\r\n",
